{
  "name": "securianpreretirecalculatorts",
  "version": "1.0.0",
  "description": "Test Framework to test Securian Pre-Retirement Calculator",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "wdio": "wdio run ./wdio.conf.ts",
    "positive_scenarios": "npx wdio ./wdio.conf.ts --suite positive_scenarios --cucumberOpts:require './test/steps/positive.scenarios.ts'",
    "negative_scenarios": "npx wdio ./wdio.conf.ts --suite negative_scenarios --cucumberOpts:require './test/steps/negative.scenarios.ts'",
    "generate_report": "allure generate allure-results --clean",
    "launch_report": "allure open"
  },
  "keywords": [
    "webdriverio",
    "nodejs",
    "cucumber",
    "typescript"
  ],
  "author": "sarodwatshome@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@wdio/allure-reporter": "^8.39.0",
    "@wdio/cucumber-framework": "^8.39.0",
    "@wdio/local-runner": "^8.39.1",
    "@wdio/spec-reporter": "^8.39.0",
    "allure-commandline": "^2.29.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "wdio-chromedriver-service": "^8.1.1"    
  },
  "dependencies": {
    "chromedriver": "^126.0.4",
    "cucumber": "^6.0.7"
  }
}
